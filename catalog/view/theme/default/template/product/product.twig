<script>
    jQuery(document).ready(function($) {
        $('#review').load('index.php?route=product/product/review&product_id={{ product_id }}');
        var price = $('#price').text().trim();
        var symbol = price.substring(0,1);
        var price_single = parseFloat(price.substr(1));

                $('.quantity-input-wrapper').click(function(e) {
                    e.preventDefault();
                    var price_x = parseFloat($('#price_x').text().trim().substr(1));

                    if ($(this).hasClass('quantity-up')) {
                        $('#input-quantity').val(parseInt($('#input-quantity').val()) + 1);
                        var total = (price_x + price_single).toFixed(2);
                        price_x = symbol + total;
                        $('#price_x').html(price_x);

                    } else {
                        if (parseInt($('#input-quantity').val()) > 1) {
                            $('#input-quantity').val(parseInt($('#input-quantity').val()) - 1);
                            var total = (price_x - price_single).toFixed(2);
                            price_x = symbol + total;
                            $('#price_x').html(price_x);
                        }
                    }
                });
        $('#elevate-zoom').elevateZoom({
            lensShape: 'basic',
            lensSize: 150,
            zoomWindowOffetx: 10,
            zoomWindowWidth: 350,
            zoomWindowHeight: 350,
            borderSize: 1,
            borderColour: '#eaeaea',
            gallery: 'product-thumbnail-gallery',
            galleryActiveClass: 'active',
            cursor:'pointer',
        });
        $('#button-cart').productInfoToCart({
            data: [
                '#product input[type=\'text\']',
                '#product input[type=\'hidden\']',
                '#product input[type=\'radio\']:checked',
                '#product input[type=\'checkbox\']:checked',
                '#product select, #product textarea'
            ],
        });
    });
    $('.desc-title').click(function (){
        var id = this.id;
        $(".desc-title-ctn").attr("class","desc-title-ctn j-desc-title-ctn desc-title-"+id);
        $('.desc-title').attr("class","desc-title j-desc-title");
        $('#'+id).attr("class","desc-title j-desc-title desc-title-active");
        $('.j-desc-con').removeClass("desc-wrap-default");
        $('.'+id).addClass("desc-wrap-default");
    });
</script>
